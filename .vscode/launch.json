{
    // Use IntelliSense to learn about possible Node.js debug attributes.
    "version": "0.2.0",
    "configurations": [
        {
            "type": "node",
            "request": "launch",
            "name": "Launch Program",
            "runtimeExecutable": "${workspaceRoot}/node_modules/.bin/babel-node",
            "cwd": "${workspaceRoot}",
            "program": "${workspaceRoot}/server.js"
        },
        {
            "type": "node",
            "request": "attach",
            "name": "Attach to Process",
            "port": 5858
        }
    ]
}